package Day_5;

interface ITest1
{
	final int num=10;// Final keyword every variable is final
	void hello();
	abstract void bye();
}
interface ITest2
{
	final int num=10;// Final keyword every variable is final
	//int hello();//-diff return type as int instead of void
	void hello();
	abstract void out();
}
class test implements ITest1,ITest2
{
	public void hello()
	{
		System.out.println("Hello");
	}
	//xpublic int hello()
	public void bye()
	{
		System.out.println("Bye");
	}
	public void in()
	{
		System.out.println("In");
	}
	
	public void out()
	{
		System.out.println("Out");
	}
}
public class Program_5_Interface {

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
//ITest obj=new ITest();--- for interface type cannot create the opject
	}

}
